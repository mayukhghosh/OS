import java.util.*;

public class Process {
	Queue<String> inst=new LinkedList<String>();
	int[] claim;
	int[] alloc;
	int[] need;
	
	//releasing n-resources where res is type of resource
	Process(int c,int a, int n)
	{
		claim=new int[c];
		alloc=new int[a];
		need=new int[n];
	}
	
	public void release_res(int res, int n)
	{
		alloc[res]=alloc[res]-n;
	}
	
	public void request_res()
	{
		
	}

}
